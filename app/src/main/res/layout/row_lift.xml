<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_marginBottom="@dimen/inner_margins">
    <TextView android:id="@+id/date_cell_text_view"
        android:text="@string/generic_title"
        style="@style/LiftHistoryTableCell" />
    <TextView android:id="@+id/rep_cell_text_view"
        android:text="@string/generic_title"
        style="@style/LiftHistoryTableCell" />
    <TextView android:id="@+id/weight_cell_text_view"
        android:text="@string/generic_title"
        style="@style/LiftHistoryTableCell" />
